<?xml version="1.0" encoding="UTF-8"?>
<suite name="NopCommerce">
	<test name="Run on Firefox">
		<parameter name="browser" value="firefox" />
		<classes>
			<class name="com.nopcommerce.user.User_02_MyAccount" />
			<class name="com.nopcommerce.user.User_04_MultiBrowser" />
		</classes>
